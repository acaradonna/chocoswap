{
  "filter_paths": "contracts/(node_modules|test)",
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "exclude_dependencies": true,
  "exclude_optimization": false,
  "disable_color": false,
  "detectors_to_exclude": [
    "solc-version",
    "pragma",
    "low-level-calls"
  ],
  "compilation_unit_prefix": "contracts/",
  "hardhat_artifacts_directory": "contracts/artifacts"
}